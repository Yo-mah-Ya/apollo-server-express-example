{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^16.13.1",
    "yarn": "^1.22"
  },
  "scripts": {
    "all": "yarn clean && yarn compile && yarn start",
    "compile": "tsc -b",
    "clean": "rimraf ./build && rimraf ./coverage && rimraf tsconfig.tsbuildinfo",
    "codegen": "graphql-codegen --config src/resolvers/codegen.yml",
    "lint": "yarn eslint && yarn prettier",
    "eslint": "eslint --fix './{src,test}/**/*.{ts,tsx}'",
    "prettier": "prettier --write './{src,test}/**/*.{ts,tsx}'",
    "start": "node --max-http-header-size=24820 ./build/src/index.js",
    "test": "jest --coverage --passWithNoTests --maxWorkers=7",
    "ts-node": "ts-node",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile"
  },
  "main": "build/src/index.js",
  "dependencies": {
    "apollo-datasource": "3.3.0",
    "apollo-datasource-rest": "3.4.0",
    "apollo-link": "1.2.14",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "apollo-link-http-common": "0.2.16",
    "apollo-server-express": "3.5.0",
    "apollo-tracing": "0.15.0",
    "aws-sdk": "2.1082.0",
    "compression": "1.7.4",
    "dataloader": "2.0.0",
    "express": "4.17.1",
    "fp-ts": "2.11.8",
    "graphql": "16.1.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-tag": "2.12.6",
    "graphql-tools": "8.2.0",
    "helmet": "4.6.0",
    "io-ts": "2.2.16",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.1.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/fragment-matcher": "3.2.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-document-nodes": "2.2.1",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.0",
    "@graphql-codegen/typescript-graphql-request": "4.3.2",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-resolvers": "2.4.2",
    "@types/compression": "1.7.2",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@types/qs": "6.9.7",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  }
}
